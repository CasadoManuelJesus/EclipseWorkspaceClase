package Ejercicio3;

public class HiloFichero {
	public static void main(String[] args) {
		Hilo hiloPrioridadMaxima = new Hilo();
		hiloPrioridadMaxima.setPriority(MAX_PRIORITY);
		hiloPrioridadMaxima.setName("MAX PRIORIDAD");
		
		Hilo hiloNormal=new Hilo();
		hiloNormal.setPriority(NORM_PRIORITY);
		hiloNormal.setName("NORM PRIORIDAD");
		
		Hilo hiloSegundoPlano = new Hilo();
		hiloSegundoPlano.setPriority(MIN_PRIORITY);
		hiloSegundoPlano.setName("MIN PRIORIDAD");
		
		hiloPrioridadMaxima.start();
		hiloNormal.start();
		hiloSegundoPlano.start();
		
		
	}
}
